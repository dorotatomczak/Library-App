<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Library">

    <Color x:Key="BackgroundLight">#efefef</Color>
    <SolidColorBrush x:Key="BackgroundLightBrush" Color="{StaticResource BackgroundLight}" />

    <Color x:Key="Burgundy">#A0041E</Color>
    <SolidColorBrush x:Key="BurgundyBrush" Color="{StaticResource Burgundy}" />

    <Color x:Key="Beige">#F3CB84</Color>
    <SolidColorBrush x:Key="BeigeBrush" Color="{StaticResource Beige}" />

</ResourceDictionary>